{
  "name": "node-typescript-basic-template",
  "version": "1.1.0",
  "main": "index.js",
  "repository": "https://github.com/DestroyeerU/node-typescript-basic-template.git",
  "author": "DestroyeerU <destroyeeru18@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions .js,.ts --out-dir dist --copy-files --no-copy-ignored",
    "start": "node ./dist/server.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --no-notify --ignore-watch node_modules src/server.ts"
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/express": "^4.17.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  }
}
